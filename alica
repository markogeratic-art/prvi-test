import sys, re
from urllib.request import urlopen
import ssl


alice = 'https://www.gutenberg.org/files/11/11-0.txt'
context = ssl.SSLContext()
Client=urlopen(alice, context=context)
lines=Client.read()
Client.close()
lines=lines.decode('utf-8')
lines=lines.split('\n')
lines=[x.strip() for x in lines]

begin = lines.index('CHAPTER I.')
end = lines.index('THE END')

lines = lines[begin:end]
words = {}
for line in lines:
    line = line.lower()
    line = line.replace('--', ' ')
    line = re.sub(r"[^a-z ]",r"", line)
    
    ws = line.split()
    for w in ws:
        words[w] = words.get(w,0) + 1

for w in sorted(words.keys()):
    print(w, words[w])